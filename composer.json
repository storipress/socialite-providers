{
  "name": "storipress/socialite-providers",
  "description": "Extra providers for Laravel Socialite",
  "license": "MIT",
  "type": "library",
  "require": {
    "php": "^8.1",
    "illuminate/events": "^10.0",
    "illuminate/support": "^10.0",
    "socialiteproviders/manager": "^4.4"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.37",
    "laravel/pint": "^1.13",
    "nunomaduro/larastan": "^2.6",
    "orchestra/testbench": "^8.11",
    "pestphp/pest-plugin-laravel": "^2.2",
    "pestphp/pest-plugin-type-coverage": "^2.2"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Storipress\\SocialiteProviders\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "pestphp/pest-plugin": true
    },
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Storipress\\SocialiteProviders\\SocialiteProvidersServiceProvider"
      ]
    }
  }
}
