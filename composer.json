{
  "name": "storipress/socialite-providers",
  "description": "Extra providers for Laravel Socialite",
  "license": "MIT",
  "type": "library",
  "require": {
    "php": "^8.1",
    "illuminate/events": "^10.0 || ^11.0",
    "illuminate/support": "^10.0 || ^11.0",
    "socialiteproviders/manager": "^4.5"
  },
  "require-dev": {
    "ergebnis/composer-normalize": "^2.42",
    "larastan/larastan": "^2.9",
    "laravel/pint": "^1.14",
    "orchestra/testbench": "^8.22 || ^9.0",
    "pestphp/pest-plugin-laravel": "^2.3",
    "pestphp/pest-plugin-type-coverage": "^2.8"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Storipress\\SocialiteProviders\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "pestphp/pest-plugin": true
    },
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Storipress\\SocialiteProviders\\SocialiteProvidersServiceProvider"
      ]
    }
  }
}
